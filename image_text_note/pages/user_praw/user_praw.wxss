/* pages/user_praw/user_praw.wxss */
page{
  height: 100%;
}
.praw{
  width: 100%;
  padding: 20rpx 32rpx 32rpx 32rpx;
  box-sizing: border-box;
  font-family:PingFang SC;
}
.search_top{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 32rpx;
}
.search_tleft{
  width: 598rpx;
  position: relative;
}
.search_inp{
  background:rgba(244,244,244,1);
  border-radius:40rpx;
  font-size:28rpx;
  font-weight:300;
  line-height:40rpx;
  color:#333;
  padding: 12rpx 0 12rpx 25rpx;
  
}
.search_tright{
  font-size:28rpx;
  font-weight:400;
  line-height:40rpx;
  color:rgba(102,102,102,1);
}
.search_sc{
  width: 60rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  right: 0rpx;
  z-index: 33;
}
.search_sc image{
  width: 30rpx;
  height: 30rpx;
}
.praw_bottom{
  width: 100%;
  display: flex;
   flex-wrap: wrap;
}
.praw_item{
  background: burlywood;
  padding: .5em;
  margin: 15rpx;
  border-radius: 12rpx;
}